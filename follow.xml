      <componentListString name="dlp-asml-internal_domains_excluding_top_secret" />
    </list>
  </in>
  <in op="like" match="any">
    <evtDomain/>
    <list>
      <componentListString name="dlp-asml-approveduploadlocations_gmf_sat_packages" />
    </list>
  </in>
</or>


